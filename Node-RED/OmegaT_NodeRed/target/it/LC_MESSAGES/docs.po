#: ../../source/docs/user-guide/writing-functions.md:490
msgid "`context`"
msgstr ""

#: ../../source/docs/user-guide/writing-functions.md:491
msgid "`context.get(..)` : get a node-scoped context property"
msgstr ""

#: ../../source/docs/user-guide/writing-functions.md:492
msgid "`context.set(..)` : set a node-scoped context property"
msgstr ""

#: ../../source/docs/user-guide/writing-functions.md:493
msgid ""
"`context.keys(..)` : return a list of all node-scoped context property "
"keys"
msgstr ""

#: ../../source/docs/user-guide/writing-functions.md:494
msgid "`context.flow` : same as `flow`"
msgstr ""

#: ../../source/docs/user-guide/writing-functions.md:495
msgid "`context.global` : same as `global`"
msgstr ""

#: ../../source/docs/user-guide/writing-functions.md:498
msgid "`flow.get(..)` : get a flow-scoped context property"
